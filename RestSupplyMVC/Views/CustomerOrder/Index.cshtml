@model List<RestSupplyMVC.ViewModels.CustomerOrderViewModel>

@{
    ViewBag.Title = "Customer Orders";
}

    <h4>Customer Orders</h4>
@if (Model.Any())
{
    foreach (var item in Model)
    {
        @Html.HiddenFor(modelItem => item.KitchenId)
        <dl class="dl-horizontal">
            <dt>
                @Html.DisplayNameFor(modelItem => item.KitchenName)
            </dt>

            <dd>
                @Html.DisplayFor(modelItem => item.KitchenName)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => item.Date)
            </dt>

            <dd>
                @Html.DisplayFor(model => item.Date)
            </dd>
            <dd>

                @Html.ActionLink("Details", "Details", "SupplierOrder", new { id = item.CustomerOrderId }, new { @id = "display-link" }) @*|
                @Html.ActionLink("Delete", "Delete", "SupplierOrder", new { id = item.CustomerOrderId }, new { @id = "delete-link" })*@
            </dd>
        </dl>
    }
}

