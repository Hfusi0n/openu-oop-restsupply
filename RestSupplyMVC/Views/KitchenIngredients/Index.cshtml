@model IEnumerable<RestSupplyDB.Models.Kitchen.KitchenIngredients>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.IngredientsSet.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.KitchensSet.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Valid)
        </th>
