        </th>
        <th>
            @Html.DisplayNameFor(model => model.CurrentQuantity)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.IngredientsSet.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.KitchensSet.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Valid)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MinimalQuantity)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CurrentQuantity)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
